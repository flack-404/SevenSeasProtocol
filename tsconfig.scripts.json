{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./dist/scripts"
  },
  "include": ["scripts/**/*.ts"],
  "exclude": ["node_modules", "app", "lib"]
}
